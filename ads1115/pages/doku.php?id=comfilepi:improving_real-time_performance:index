<!-- Do not translate code -->
<script language="javascript">
// Don't translate code
function noTranslate(elements)
{
  var i;

  for(i = 0; i < elements.length; i++) 
  {
    // translate="no" does not seem to be supported by any browser yet, but it's here just in case.
    var a = document.createAttribute("translate");
    a.value = "no";
    elements[i].setAttributeNode(a);
    
    // This works.  Add a notranslate class
    elements[i].className += " notranslate";
  }
}

noTranslate(document.getElementsByTagName("code"));
noTranslate(document.getElementsByTagName("pre"));
</script>

