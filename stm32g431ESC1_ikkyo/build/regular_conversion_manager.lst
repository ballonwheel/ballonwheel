 170:Src/regular_conversion_manager.c ****   if (regConv->convHandle < RCM_MAX_CONV)
 171:Src/regular_conversion_manager.c ****   {
 172:Src/regular_conversion_manager.c ****     RCM_CB_array [regConv->convHandle].cb = fctCB;
 173:Src/regular_conversion_manager.c ****     RCM_CB_array [regConv->convHandle].data = data;
 174:Src/regular_conversion_manager.c ****   }
